<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Actions</key>
	<array>
		<dict>
			<key>Image File</key>
			<string>mplayerx.png</string>
			<key>Regular Expression</key>
			<string>(.*youtube\.com.*|.*vimeo\.com.*)</string>
			<key>Requirements</key>
			<array>
				<string>httpurls</string>
			</array>
			<key>Service Name</key>
			<string>MPlayerX: Open URL</string>
			<key>Title</key>
			<string>MPlayerX</string>
		</dict>
	</array>
	<key>Apps</key>
	<array>
		<dict>
			<key>Bundle Identifier</key>
			<string>org.niltsh.MPlayerX</string>
			<key>Check Installed</key>
			<true/>
			<key>Link</key>
			<string>http://mplayerx.org</string>
			<key>Name</key>
			<string>MPlayerX</string>
		</dict>
	</array>
	<key>Extension Description</key>
	<string>Open selected URL with MPlayerX.</string>
	<key>Extension Identifier</key>
	<string>hu.junfeng.mplayerx</string>
	<key>Extension Name</key>
	<string>MPlayerX</string>
	<key>Version</key>
	<integer>2</integer>
</dict>
</plist>
